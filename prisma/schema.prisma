generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Screenshot {
  id          String   @id @default(cuid())
  filename    String
  originalName String
  url         String
  size        Int
  mimeType    String
  tags        String[]
  createdAt   DateTime @default(now())

  @@index([createdAt])
  @@index([tags])
}
